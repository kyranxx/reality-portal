{
  "name": "reality-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "node scripts/validate-environment.js",
    "dev": "next dev -p 3002",
    "dev:turbo": "next dev --turbo -p 3002",
    "dev:with-turbo": "node scripts/validate-environment.js && next dev --turbo -p 3002",
    "dev:fast": "next dev -p 3002",
    "dev:quick": "node scripts/quick-start.js",
    "dev:emulators": "node scripts/start-dev-with-emulators.js",
    "validate": "node scripts/pre-build-validation.js",
    "validate:deploy": "node scripts/validate-deployment.js",
    "validate:cors": "node scripts/validate-cors.js",
    "validate:env": "node scripts/validate-environment.js",
    "prebuild": "node scripts/validate-environment.js && node prebuild.js",
    "build": "npm run validate && npm run prebuild && next build",
    "build:vercel": "npm run validate && npm run validate:deploy && npm run validate:cors && node build-vercel.js",
    "start": "next start -p 3002",
    "lint": "next lint",
    "clean": "rimraf .next out",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@firebase/auth": "^1.5.1",
    "@next/bundle-analyzer": "^15.2.2",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "firebase": "^10.7.0",
    "glob": "^9.3.5",
    "i18next": "^24.2.3",
    "next": "^14.1.0",
    "next-i18next": "^15.4.2",
    "postcss": "^8.4.32",
    "puppeteer": "^24.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.4.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@eslint/config-array": "^0.19.2",
    "@eslint/object-schema": "^2.1.6",
    "chalk": "^4.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.23.0",
    "eslint-config-next": "^14.1.0",
    "ignore-loader": "^0.1.2",
    "lru-cache": "^11.0.2",
    "null-loader": "^4.0.1",
    "prettier": "^3.5.3",
    "rimraf": "^4.4.1"
  }
}
