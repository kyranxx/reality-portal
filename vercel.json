{
  "buildCommand": "npm run build:vercel",
  "framework": "nextjs",
  "outputDirectory": ".next-dynamic",
  "installCommand": "npm install",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_IS_VERCEL": "true"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm run build:vercel",
        "skipAutoInstall": false,
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "build": {
    "env": {
      "VERCEL": "1",
      "VERCEL_ENV": "production"
    }
  }
}
